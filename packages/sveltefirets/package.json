    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "svelte2tsx": "^0.5.9",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
